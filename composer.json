{
  "name": "folour/flavy",
  "description": "FFmpeg layer for Laravel 5.2-5.4, this is a fork of rafasamp/sonus package",
  "keywords": ["laravel", "ffmpeg", "audio", "video", "converter", "conversion", "video thumbnailer"],
  "minimum-stability": "stable",
  "license": "GPLv3",
  "authors": [
    {
      "name": "Vadim Bova",
      "email": "folour@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6",
    "laravel/framework": ">=5.2"
  },
  "autoload": {
    "psr-4": {
      "Folour\\Flavy\\": "src/Folour/Flavy"
    }
  },
  "extra": {
        "laravel": {
            "providers": [
                "Folour\\Flavy\\Provider\\FlavyServiceProvider"
            ],
            "aliases": {
                "Flavy": "Folour\\Flavy\\FlavyFacade"
            }
        }
    },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "zendframework/zend-coding-standard": "^1.0"
  },
  "suggest": {
    "ext-xdebug": "^2.6.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test-coverage": "phpunit --coverage-clover clover.xml"
  }
}
